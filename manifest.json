{
  "name": "MyExtension",
  "version": "1.0",
  "minimum_chrome_version": "10.0",
  "manifest_version": 2,
  "description": "Learn how to make devtools.",
  "devtools_page": "devtools.html",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}